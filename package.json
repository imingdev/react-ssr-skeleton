{
  "name": "react-ssr-skeleton",
  "version": "1.0.0",
  "author": "mingdev <mingdev@163.com>",
  "scripts": {
    "build:client": "node_modules/.bin/webpack --inline --progress --config build/webpack.client.config.js",
    "build:server": "node_modules/.bin/webpack --inline --progress --config build/webpack.server.config.js",
    "build:prod": "cross-env NODE_ENV=production npm run build:client && cross-env NODE_ENV=production npm run build:server",
    "build:dev": "cross-env NODE_ENV=development npm run build:client && cross-env NODE_ENV=development npm run build:server"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "dotenv-webpack": "^2.0.0",
    "file-loader": "^6.0.0",
    "ignore-loader": "^0.1.2",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^0.11.0",
    "node-sass": "^4.14.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-url": "^8.0.0",
    "sass-loader": "^10.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dynamic-entry-plugin": "^1.0.3",
